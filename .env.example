# Swagger 输入源（URL 或本地文件路径）
SWAGGER_INPUT=https://api.example.com/swagger.json
# SWAGGER_INPUT=./swagger.json

# 输出目录
OUTPUT_PATH=./dist/api

# API 基础 URL（可选，用于客户端配置）
API_BASE_URL=https://api.example.com

# 是否将 Swagger 2.0 转换为 OpenAPI 3.x
CONVERT_TO_V3=true

# 是否启用增量缓存
ENABLE_CACHE=true

# 是否清空输出目录
CLEAN_OUTPUT=false
