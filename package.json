{
  "name": "@miaoosi/swagger2ts",
  "version": "0.3.3",
  "type": "module",
  "description": "A developer-friendly CLI wrapper for Kubb with multi-source management, smart caching, and Swagger 2.0 support",
  "author": "Chaos",
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "swagger2ts",
    "api-generator",
    "code-generator",
    "kubb"
  ],
  "bin": {
    "swagger2ts": "./dist/bin/swagger2ts.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/knight174/swagger2ts.git"
  },
  "bugs": {
    "url": "https://github.com/knight174/swagger2ts/issues"
  },
  "homepage": "https://github.com/knight174/swagger2ts#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
